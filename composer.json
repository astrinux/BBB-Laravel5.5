{
    "name": "djoudi/bbb-laravel5.5",
    "description": "Bigbluebutton wrapper for laravel 5.5",
    "license": "MIT",
    "keywords": ["laravel", "bigbluebutton", "wrapper"],
    "authors": [
        {
            "name": "Abdelouahab Djoudi",
            "email": "info@djoudi.net"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.0",
        "laravel/framework": "5.5.*",
        "update-bigbluebutton/bigbluebutton-api-php": "^1.0"

    },
    "autoload": {
        "psr-4": {
            "Djoudi\\Bigbluebutton\\":"src/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
            }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
